#include <iostream>
using namespace std;

int main()
{
    int n;

    cout << "Kode: ";
    cin >> n;

    if (n <= 0) 
    {
        cout << "bukan" << endl;
    }

    while (n % 2 == 0) 
    {
        n/=2;
    }
    
    if (n == 1)
    {
        cout << "ya" << endl;
    }
    else
    { 
        cout << "bukan" << endl;
    }
}
